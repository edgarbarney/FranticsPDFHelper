<Window x:Class="Frantics_PDF_Helper.Windows.SandboxWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frantics_PDF_Helper.Windows"
        mc:Ignorable="d"
        
        Title="MainWindow"
        Width="1920"
        Height="1080"
        WindowStyle="None"
        WindowState="Maximized"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize">
    <Grid Background="White">
        <Canvas x:Name="drawCanvas" MouseDown="DrawCanvas_MouseDown" MouseMove="DrawCanvas_MouseMove" Background="Transparent">
            
        </Canvas>
    </Grid>
</Window>
